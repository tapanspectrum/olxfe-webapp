{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tap-admin": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "tap",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/tap-admin/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "src/styles.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/assets/themes/css/default.css",
              "src/assets/themes/css/style.css",
              "src/assets/themes/css/slick.css",
              "src/assets/themes/css/nice-select.css",
              "src/assets/themes/css/magnific-popup.css",
              "src/assets/themes/css/ion.rangeSlider.min.css",
              "src/assets/themes/css/fontawesome.min.css",
              "src/assets/themes/css/bootstrap.min.css",
              "src/assets/themes/scss/style.scss"
            ],
            "scripts": [
              "src/assets/themes/js/vendor/jquery-1.12.4.min.js",
              "src/assets/themes/js/vendor/modernizr-3.7.1.min.js",
              "src/assets/themes/js/popper.min.js",
              "src/assets/themes/js/bootstrap.min.js",
              "src/assets/themes/js/slick.min.js",
              "src/assets/themes/js/jquery.magnific-popup.min.js",
              "src/assets/themes/js/jquery.nice-select.min.js",
              "src/assets/themes/js/waypoints.min.js",
              "src/assets/themes/js/jquery.counterup.min.js",
              "src/assets/themes/js/ion.rangeSlider.min.js",
              "src/assets/themes/js/ajax-contact.js",
              "src/assets/themes/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "tap-admin:build:production"
            },
            "development": {
              "browserTarget": "tap-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tap-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "tap-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tap-admin:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/tap-admin/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media"
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "tap-admin:build:development",
              "serverTarget": "tap-admin:server:development"
            },
            "production": {
              "browserTarget": "tap-admin:build:production",
              "serverTarget": "tap-admin:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "tap-admin:build:production",
              "serverTarget": "tap-admin:server:production"
            },
            "development": {
              "browserTarget": "tap-admin:build:development",
              "serverTarget": "tap-admin:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "cli": {
    "analytics": "92282fee-0b08-49aa-87c4-86edb19819d4"
  }
}
